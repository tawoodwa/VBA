Private Sub attendeedatatoggle_Click()
Dim cf As Worksheet
Set cf = Worksheets("Configuration")
If attendeedatatoggle.Value = True Then
    cf.Range("B53").Value = 1
    cf.Range("B69").Value = 1
    attendeedatatoggle.BackColor = RGB(255, 0, 0)
ElseIf attendeedatatoggle.Value = False Then
    cf.Range("B53").Value = 0
    cf.Range("B69").Value = 0
    attendeedatatoggle.BackColor = &H8000000F
End If
End Sub

Private Sub CommandButton2_Click()
Call clean
End Sub

Private Sub eventdatatoggle_Click()
Dim cf As Worksheet
Set cf = Worksheets("Configuration")
